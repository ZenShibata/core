    "@sapphire/pieces": "^3.6.3",
    "@sapphire/result": "^2.6.4",
    "@sapphire/snowflake": "^3.5.1",
    "@sapphire/utilities": "^3.11.2",
    "amqp-connection-manager": "^4.1.13",
    "discord-api-types": "^0.37.42",
    "ioredis": "^5.3.2",
    "tslib": "^2.5.0"
